def get_most_common_enemy(enemies_dict):
## this is the dictionary that was used for testing. enemies_dict = {'jackal': 4, 'kobold': 3, 'soldier': 10, 'gremlin': 5}
    max_so_far = float("-inf")
    max_key = None

    print (enemies_dict)
    for key, value in enemies_dict.items():
      #iterate over the keys and values
        if value > max_so_far:
            max_so_far = value
            max_key = key
      #update each max_so_far with the highest value, and max_key with the name of the primary key.
      
    return max_key
